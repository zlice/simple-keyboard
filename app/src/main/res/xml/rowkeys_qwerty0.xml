<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case
            latin:showExtraChars="true"
            latin:keyboardLayoutSetElement="alphabetManualShifted">
            <include
                latin:keyboardLayout="@xml/key_styles_currency" />

            <Key
                latin:keySpec="!"
                latin:moreKeys="!text/morekeys_exclamation" />
            <Key
                latin:keySpec="\@" />
            <Key
                latin:keySpec="\#" />
            <Key
                latin:keyStyle="currencyKeyStyle"
                latin:keyLabelFlags="followKeyLetterRatio" />
            <Key
                latin:keySpec="!text/keyspec_symbols_percent"
                latin:moreKeys="!text/morekeys_symbols_percent" />
            <Key
                latin:keySpec="^" />
            <Key
                latin:keySpec="&amp;" />
            <Key
                latin:keySpec="*"
                latin:moreKeys="!text/morekeys_star" />
            <include
                latin:keyboardLayout="@xml/keys_parentheses" />
        </case>
        <default>
            <Key
                latin:keySpec="1"
                latin:keyHintLabel="!"
                latin:additionalMoreKeys="!" />
            <Key
                latin:keySpec="2"
                latin:keyHintLabel="\@"
                latin:additionalMoreKeys="\@" />
            <Key
                latin:keySpec="3"
                latin:keyHintLabel="\#"
                latin:additionalMoreKeys="\#" />
            <Key
                latin:keySpec="4"
                latin:keyHintLabel="\$"
                latin:additionalMoreKeys="\$" />
            <Key
                latin:keySpec="5"
                latin:keyHintLabel="!text/keyspec_symbols_percent"
                latin:additionalMoreKeys="!text/keyspec_symbols_percent" />
            <Key
                latin:keySpec="6"
                latin:keyHintLabel="^"
                latin:additionalMoreKeys="^" />
            <Key
                latin:keySpec="7"
                latin:keyHintLabel="&amp;"
                latin:additionalMoreKeys="&amp;" />
            <Key
                latin:keySpec="8"
                latin:keyHintLabel="*"
                latin:additionalMoreKeys="*" />
            <Key
                latin:keySpec="9"
                latin:keyHintLabel="!text/keyspec_left_parenthesis"
                latin:additionalMoreKeys="!text/keyspec_left_parenthesis" />
            <Key
                latin:keySpec="0"
                latin:keyHintLabel="!text/keyspec_right_parenthesis"
                latin:additionalMoreKeys="!text/keyspec_right_parenthesis" />
        </default>
    </switch>
</merge>
